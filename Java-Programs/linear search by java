class LSearch {
	static int search(int arr[], int n, int z )
	{
		for (int i = 0; i < n; i++) {
			if (arr[i] == z)
				return i;
		}
		return -1;
	}
    public static void main(String[] args)
	{
		int[] arr = { 3, 4, 1, 7, 5 };
		int n = arr.length;
		int z = 4;
    int index = search(arr, n, z);
		if (index == -1)
			System.out.println("elements are not in array");
		else
			System.out.println("Element searched " + index);
	}
}
